{
    "functions":{
        // the ai can create its own functions and add them too 
        "open_new_tab_chrome":{
            inputs_required: {}
            description: "this function is used to open a new tab in chrome"
            execution:{
                "ctrl_shortcuts": "ctrl + t"
            }
        }
    }
    "execution":{
        // this is the sequence in which the ai will execute the functions
        "sequence":[
            "open_app":{"chrome"},
            "tap":{
                ctrl_shortcuts: "ctrl + l"
