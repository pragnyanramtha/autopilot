            },
            "type":{"jeff_bezos"},
            "tap":{"enter_key"}
        ]
    }
   
}


// this is a map of all the existing functions that have to be sent to the ai along with the prompt 
{
    "functions":{
        "open_app":{
            // a second level function given by us 
            "inputs_required": {"app_name":str}
            "description": "this function is used to open the app specified"
            "execution":{
                "tap": "win_key"
                "delay": 0.5
                "type": "{app_name}"
                "tap": "enter_key"
            }
        // primary functions 
        "tap":{
            "inputs_required": {"key":str}
            "description": "this function is used to tap the specified key"
            "execution":{
                "tap": "{key}"
            }
        }
        "type":{
            "inputs_required": {"text":str}
            "description": "this function is used to type the specified text"
            "execution":{
                "type": "{text}"
            }
        }
        "delay":{
            "inputs_required": {"time":float}
            "description": "this function is used to delay the execution for the specified time"
            "execution":{
                "delay": "{time}"
            }
        "ctrl_shortcuts":{
            //lets say what ever input it takes it will execute the shortcut ,with ctrl + input 
            "inputs_required": {"shortcut":str}
            "description": "this function is used to execute the specified shortcut"
            "execution":{
                "shortcut": "{shortcut}"
            }
            
        }
